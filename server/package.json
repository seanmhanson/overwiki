{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "engines": {
    "node": ">=8.10.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "build": "tsc",
    "pretty-quick": "pretty-quick --staged",
    "format": "prettier --write src test",
    "lint": "eslint . --ext .ts",
    "start": "node .",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick && npm run lint"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.11",
    "nodemon": "^2.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "typescript": "^3.8.3"
  }
}
